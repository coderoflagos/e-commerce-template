<!DOCTYPE html>
<html>

<head>
    {{! Use the formatter.title and formatter.description in the head }}
    <title>{{page.head.title}} | {{site.info.title}}</title>
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name='viewport' content='width=device-width, initial-scale=1.0'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="HandheldFriendly" content="true">
    {{#if page.head.description}}
    {{! Only render the description if it exists }}
    <meta name="description" content="{{page.head.description}}">
    {{/if}}

    <!--Font-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <!-- Twitter Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We" crossorigin="anonymous">

    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj"
        crossorigin="anonymous"></script>

    <link rel="stylesheet" href="/static/css/allwomen.css">

    <style>

    </style>
</head>

<body>

  <header>
      <div class="container">
        <a href="" class="navlink">
          <h2 style="font-size: 25px;">
            {{#each (blogs site.pages "/content/kids/")}}
            {{ page.head.extra.navtitle }}
            {{/each}}

          </h2>
        </a>
        <nav>
          <a href="/cart"><i class="fa fa-shopping-bag" aria-hidden="true"></i></a>

        </nav>
        <a class="hamburger" href="/cart">
          <i class="fa fa-shopping-bag"></i>
        </a>
      </div>
    </header>
    <nav class="mobile-nav">
      <a href="/checkout" style="	padding: 5px"><i class="fa fa-shopping-bag" aria-hidden="true"></i></a>
    </nav>


    <br><br><br><br><br><br><br><br>



    <style>
     
    </style>

    <div class="container-mailing">
        <div class="row">

            <h2 style="text-align: left;">
                {{ page.head.extra.category }}
            </h2>

            <hr style="  border: 1.5px solid #ffffff; border-radius: 10px;
    width: 190px;">
        </div>
    </div>

    <div class="container-mailing">

        <div class="row">
            {{#each (blogs site.pages "/content/kids/")}}
            <div class="col-md-4">
                <p align="center">
                    <a href="{{uri}}">
                        <div class="cardo">
                            <main class="badge">Hot</main>
                            <div class="cardo_tumb">
                                <img src="{{page.head.extra.photo-src}}" alt="" width="100%">
                            </div>
                            <div class="cardo_details">
                                <span class="cardo_catagory">{{page.head.extra.tag}}</span>
                                <h4><a href="">{{page.head.title}}</a></h4>
                                <p>{{page.head.extra.product_description}}</p>
                                <div class="cardo_bottom-details">
                                    <div class="cardo_price">{{page.head.extra.price}}</div>
                                    <div class="cardo_links">
                                        <a href=""><i class="fa fa-heart-o"></i></a>
                                        <a href=""><i class="fa fa-shopping-cart"></i></a>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </a>
                </p>
            </div>
            {{/each}}

        </div>
    </div>
    <!-- end col -->

   
    {{> content_bottom }}