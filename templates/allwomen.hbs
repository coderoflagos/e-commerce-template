<style>
    body {
        background-color: #000;
        color: #fff;
    }

    @import url('https://fonts.googleapis.com/css?family=Roboto:400,500,700');

    * {
        -webkit-box-sizing: border-box;
        box-sizing: border-box;
        margin: 0;
        padding: 0;
    }


    body {
        font-family: 'Roboto', sans-serif;
    }

    a {
        text-decoration: none;
    }

    .cardo {
        width: 100%;
        position: relative;
        margin: 10px auto;
        background: #000;
        border: 2px solid #fff;
    }

    .badge {
        position: absolute;
        left: 0;
        text-transform: uppercase;
        font-size: 13px;
        font-weight: 700;
        background: red;
        color: #fff;
        border-radius: 0px;
    }

    .cardo_tumb {
        display: flex;
        align-items: center;
        justify-content: center;
        height: 300px;
        background: #000;
    }

    .cardo_tumb img {
        max-width: 100%;
        max-height: 100%;
    }

    .cardo_details {
        padding: 30px;
    }

    .cardo_catagory {
        display: block;
        font-size: 12px;
        font-weight: 700;
        text-transform: uppercase;
        color: #ccc;
        margin-bottom: 18px;
    }

    .cardo_details h4 a {
        font-weight: 500;
        display: block;
        margin-bottom: 18px;
        text-transform: uppercase;
        color: #363636;
        text-decoration: none;
        transition: 0.3s;
    }

    .cardo_details h4 a:hover {
        color: #fbb72c;
    }

    .cardo_details p {
        font-size: 15px;
        line-height: 22px;
        margin-bottom: 18px;
        color: #999;
    }

    .cardo_bottom-details {
        overflow: hidden;
        border-top: 1px solid #eee;
        padding-top: 20px;
    }

    .cardo_bottom-details div {
        float: left;
        width: 50%;
    }

    .cardo_price {
        font-size: 18px;
        color: #fbb72c;
        font-weight: 600;
    }

    .cardo_price small {
        font-size: 80%;
        font-weight: 400;
        text-decoration: line-through;
        display: inline-block;
        margin-right: 5px;
    }

    .cardo_links {
        text-align: right;
    }

    .cardo_links a {
        display: inline-block;
        margin-left: 5px;
        color: #e1e1e1;
        transition: 0.3s;
        font-size: 17px;
    }

    .cardo_links a:hover {
        color: #fbb72c;
    }
</style>

